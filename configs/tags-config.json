                "3": "tag",
                "4": "character",
                "5": "group",
                "6": "language",
                "7": "category"
            },
            "multiSelectSupport": {
                "tag": true,
                "artist": true,
                "character": true,
                "parody": true,
                "group": true,
                "language": false,
                "category": false
            },
            "migration": {
                "enabled": true,
                "remoteUrl": "https://cdn.jsdelivr.net/gh/shirokun20/nhasixapp@configs/configs/tags/tags_nhentai.json",
                "fallbackUrl": "https://cdn.jsdelivr.net/gh/shirokun20/nhasixapp@master/configs/tags/tags_nhentai.json",
                "compression": "none",
                "cacheTtlSeconds": 86400
            }
        },
        "crotpedia": {
            "type": "remote",
            "format": "array",
            "structure": [
                "id",
                "name",
                "slug",
                "typeCode"
            ],
            "typeCodeMapping": {
                "8": "Genre"
            },
            "multiSelectSupport": {
                "tag": true
            },
            "migration": {
                "enabled": true,
                "remoteUrl": "https://cdn.jsdelivr.net/gh/shirokun20/nhasixapp@configs/configs/tags/tags_crotpedia.json",
                "compression": "none",
                "cacheTtlSeconds": 86400
            }
        }
    },
    "notes": [
